      "typeVersion": 4.1,
      "position": [
        460,
        300
      ],
      "credentials": {
        "microsoftOAuth2Api": {
          "id": "5ZmmOyK1L7PhkJW2",
          "name": "Microsoft SharePoint account"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "const items = $input.first().json.value || [];\n\nconst massnahmen = items.map(item => {\n  const f = item.fields || {};\n  return {\n    id: item.id,\n    nzaId: f.NZA_ID,\n    titel: f.Title,\n    typ: f.Massnahme_Typ,\n    beschreibung: f.Beschreibung,\n    verantwortlichId: f.Verantwortlich_ID,\n    verantwortlichName: f.Verantwortlich_Name,\n    verantwortlichEmail: f.Verantwortlich_Email,\n    termin: f.Termin,\n    status: f.Status || 'Offen',\n    erledigtAm: f.Erledigt_Am,\n    wirksamkeit: f.Wirksamkeit,\n    benachrichtigungTeams: f.Benachrichtigung_Teams === true,\n    benachrichtigungEmail: f.Benachrichtigung_Email === true,\n    erstelltAm: item.createdDateTime\n  };\n});\n\nreturn [{ json: { massnahmen, count: massnahmen.length } }];"
      },
      "id": "transform",
      "name": "Transform",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
