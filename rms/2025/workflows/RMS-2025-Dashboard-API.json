    },
    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst query = $('Webhook Reklamationen').first().json.query || {};\n\nlet result = items.map(item => {\n  const f = item.json.fields || item.json;\n  return {\n    id: item.json.id,\n    qa_id: f.QA_ID || f.Title,\n    typ: f.Rekla_Typ,\n    absender: f.Absender,\n    artikel: f.Artikel_Nr,\n    datum: f.Erfassungsdatum,\n    formular: f.Formular,\n    status: f.Status,\n    fehler: f.Fehler_Beschreibung\n  };\n});\n\n// Filter\nif (query.typ) {\n  result = result.filter(r => r.typ === query.typ);\n}\nif (query.search) {\n  const s = query.search.toLowerCase();\n  result = result.filter(r => \n    (r.qa_id || '').toLowerCase().includes(s) ||\n    (r.absender || '').toLowerCase().includes(s) ||\n    (r.artikel || '').toLowerCase().includes(s)\n  );\n}\n\nreturn [{ json: result }];"
      },
